{
  "name": "symfony/framework-standard-edition",
  "license": "MIT",
  "type": "project",
  "description": "The \"Symfony Standard Edition\" distribution",
  "autoload": {
    "psr-0": {
      "": "src/"
    }
  },
  "require": {
    "php": ">=5.4.4",
    "symfony/symfony": "2.7.*",
    "symfony/assetic-bundle": "~2.3",
    "symfony/swiftmailer-bundle": "~2.3",
    "symfony/monolog-bundle": "~2.4",
    "doctrine/orm": "2.5.*",
    "doctrine/doctrine-bundle": "1.5.*",
    "doctrine/doctrine-fixtures-bundle": "2.2.*",
    "twig/extensions": "1.3.*",
    "phpunit/phpunit": "4.7.*",
    "phpunit/phpunit-selenium": ">=1.2",
    "sensio/distribution-bundle": "~4.0",
    "sensio/framework-extra-bundle": "~3.0",
    "sensio/generator-bundle": "~2.5",
    "doctrine/migrations": "dev-master",
    "doctrine/doctrine-migrations-bundle": "dev-master",
    "incenteev/composer-parameter-handler": "~2.0",
    "jms/serializer": "dev-master",
    "jms/serializer-bundle": "~1.0.0",
    "egulias/email-validator": "~1.2",
    "friendsofsymfony/user-bundle": "1.3.*",
    "friendsofsymfony/rest-bundle": "1.7.*",
    "friendsofsymfony/jsrouting-bundle": "1.5.*",
    "stof/doctrine-extensions-bundle": "1.1.*",
    "willdurand/js-translation-bundle": "2.2.*",
    "leafo/lessphp": "0.4.0",
    "braincrafted/bootstrap-bundle": "2.1.2",
    "php-ffmpeg/php-ffmpeg": "0.6.*",
    "wisembly/elephant.io": "dev-master",
    "predis/predis": "~1.0.1",
    "lexik/maintenance-bundle": "dev-master",
    "hwi/oauth-bundle": "0.4.*@dev",
    "facebook/php-sdk-v4": "4.0.*",
    "richsage/rms-push-notifications-bundle": "0.1.*",
    "coresphere/console-bundle": "dev-master",
    "stfalcon/tinymce-bundle": "@stable",
    "gedmo/doctrine-extensions": "@stable",
    "rase/socket.io-emitter": "@stable",
    "liuggio/excelbundle": "@stable",
    "twbs/bootstrap": "^3.3"
  },
  "scripts": {
    "post-install-cmd": [
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Braincrafted\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::install"
    ],
    "post-update-cmd": [
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Braincrafted\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::install"
    ]
  },
  "config": {
    "bin-dir": "bin"
  },
  "minimum-stability": "stable",
  "extra": {
    "symfony-app-dir": "app",
    "symfony-web-dir": "web",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    },
    "branch-alias": {
      "dev-master": "2.3-dev"
    }
  }
}

{% endblock %}

{% block corps %}

    <div id="corps-container" class="large-container">
        <div class="container">
            <div class="container-md">
                {{ 'memoiregroupechasse.raw'|trans({'%url%': url('naturapass_main_map')},translation_name~'memoiregroupechasse') | raw }}
                <div class="row">
                    <div class="col-sm-6"><img src="{{ asset("img/interface/journal1.jpg") }}" class="img-responsive center-block"></div>

{% extends "NaturaPassMainBundle::layout-no-menu.html.twig" %}

{% block title %}{{ 'memoiregroupechasse.title'|trans({},translation_name~'memoiregroupechasse') }} | {{ parent() }}{% endblock %}
{% block description %}{{ 'memoiregroupechasse.description'|trans({},translation_name~'memoiregroupechasse') }}{% endblock %}

{% block stylesheets %}
